{% spaceless %}

    {% for entity in objectsOne %}

        {% set id = entity.id|default('< missing ID >') %}
        {% set name = entity.name_box|default('< missing name_box >') %}

        <li
            class="{{ boxOne.li_class }} x-hand box-one-truncate"
            data-id="{{ id }}"
            data-toggle="tooltip"
            data-original-title="{{ name }}"
        >

            <span class="handle ui-sortable-handle">
                <i class="fa fa-ellipsis-v"></i>
                <i class="fa fa-ellipsis-v"></i>
            </span>

            <input type="radio"
                   name="{{ boxOne.li_input_name }}"
                   value="{{ id }}">
            &nbsp;
            <span class="badge {{ boxOne.li_badge_class }}">
                {{ id }}
            </span>

            <span class="text">
                {{ name }}
            </span>

            {#<div class="tools">#}
            {#<i class="fa fa-edit"></i>#}
            {#<i class="fa fa-trash-o"></i>#}
            {#</div>#}
        </li>
    {% else %}
        <li>
            <span class="text">
                No hay resultados.
            </span>
        </li>
    {% endfor %}

{% endspaceless %}
