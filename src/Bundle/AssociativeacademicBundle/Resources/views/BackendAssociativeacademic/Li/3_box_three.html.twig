{% spaceless %}

    {% for entity in objectsThree %}

        {% set id = entity.id|default('< missing ID >') %}
        {% set name = entity.name_box|default('< missing name_box >') %}

        <li
            class="{{ boxThree.li_class }}"
            data-box-three-id="{{ id }}"
            data-toggle="tooltip"
            data-original-title="{{ name }}"
        >

            <span class="handle ui-sortable-handle">
                <i class="fa fa-ellipsis-v"></i>
                <i class="fa fa-ellipsis-v"></i>
            </span>

            <input type="checkbox"
                   name="{{ boxThree.li_input_name }}"
                   checked="checked"
                   value="{{ id }}">
            &nbsp;
            <span class="badge {{ boxThree.li_badge_class }}">
                {{ id }}
            </span>

            <span class="text box-truncate">
                {{ name }}
            </span>

            <div class="tools">
                <i class="fa fa-arrow-circle-right x-hand"></i>
            </div>
        </li>
    {% else %}
        <li>
            <span class="text">
                No hay resultados.
            </span>
        </li>
    {% endfor %}

{% endspaceless %}
