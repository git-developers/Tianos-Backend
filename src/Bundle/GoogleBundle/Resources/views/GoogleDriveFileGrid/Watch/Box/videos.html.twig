<div class="box-header with-border">
    <h3 class="box-title">
        <i class="fa fa-fw fa-youtube-play"></i> Videos relacionados
    </h3>
    <div class="box-tools">
        <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip" title="Colapsar">
            <i class="fa fa-minus"></i>
        </button>
    </div>
</div>
<div class="box-body">

    <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
        {#<ol class="carousel-indicators">#}
        {#{% for key, value in 1..3 %}#}
        {#<li data-target="#carousel-example-generic" data-slide-to="{{ key }}" class="{{ (key == 1) ? 'active' : '' }}"></li>#}
        {#{% endfor %}#}
        {#</ol>#}
        <div class="carousel-inner">
            {% for key, value in 1..12 %}
                <div class="item {{ (key == 1) ? 'active' : '' }}">
                    <div class="col-xs-3">
                        <img src="http://placehold.it/900x500/39CCCC/ffffff&amp;text=I+Love+Bootstrap" alt="First slide" style="height: 100px" class="img-responsive">
                        <div class="carousel-caption">First Slide -- {{ key }}</div>
                    </div>
                </div>
            {% endfor %}
        </div>
        <a class="left carousel-control" href="#carousel-example-generic" data-slide="prev">
            <span class="fa fa-angle-left"></span>
        </a>
        <a class="right carousel-control" href="#carousel-example-generic" data-slide="next">
            <span class="fa fa-angle-right"></span>
        </a>
    </div>

</div>