{% for object in objects %}
    <div class="box "> {#box-widget#}
        <div class="box-header with-border">
            <div class="user-block">
                <img
                    class="img-circle"
                    src="{{ asset('bundles/user/images/no_avatar.jpg') }}"
                    alt=""
                    data-toggle="tooltip"
                    title="Jonathan Burke Jr">
                <span class="username">
                    <a href="{{ path('frontend_user_profile', {slug: object.user_slug}) }}">
                        {{ object.user_name|slice(0, 15) }}
                    </a>
                </span>
                {#<span class="description">Shared publicly - 7:30 PM Today</span>#}
            </div>
            <div class="box-tools">
                <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip" title="Colapsar">
                    <i class="fa fa-minus"></i>
                </button>
            </div>
        </div>

        <div class="box-body" onclick="location.href='{{ path('backend_google_drive_grid_watch', { slug: object.slug }) }}';">
            <img
                data-toggle="tooltip"
                title="{{ object.file_name }}"
                class="img-responsive pad x-hand"
                src="{{ asset('bundles/themes/adminlte/images/empty-image-300x200.png') }}"
                alt="Photo">
            <p
                class="x-hand"
                data-toggle="tooltip"
                title="{{ object.file_name }}">
                {{ object.file_name|slice(0, 20) }}
            </p>
            <button
                type="button"
                class="btn btn-default btn-xs"
                data-toggle="tooltip"
                title="likes 5079">
                <i class="fa fa-thumbs-o-up"></i> {{ random(5000) }}
            </button>
            <button
                type="button"
                class="btn btn-default btn-xs"
                data-toggle="tooltip"
                title="vistas 4635">
                <i class="fa fa-eye"></i> {{ random(20000) }}
            </button>
        </div>
    </div>
{% else %}
    <div class="box-header with-border">
        No hay archivos relevantes.
    </div>
{% endfor %}
