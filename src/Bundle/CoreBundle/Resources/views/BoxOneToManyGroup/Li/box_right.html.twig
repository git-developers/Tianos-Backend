{% spaceless %}
    {% for key, entity in rightEntity %}

        <li class="x-move {{ boxRight.box_li_class }}" data-id="{{ entity.id }}">

            <span class="handle">
                <i class="fa fa-ellipsis-v"></i>
                <i class="fa fa-ellipsis-v"></i>
            </span>

            <input type="checkbox" value="">

            <span class="badge {{ boxRight.badge_class }}">
                {{ entity.id }}
            </span>

            <span class="text">
                {{ entity.name_box|default() }}
            </span>

            <div class="tools">
            <i class="fa fa-edit"></i>
            <i class="fa fa-trash-o"></i>
            </div>
        </li>
    {% else %}
        <li>
            <span class="text">
                No hay resultados.
            </span>
        </li>
    {% endfor %}
{% endspaceless %}
