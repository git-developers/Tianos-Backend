{% spaceless %}

    {% for entity in objectsLeft %}

        {% set id = entity.id|default('< missing ID >') %}

        <li class="{{ boxLeft.li_class }} x-hand" data-id="{{ id }}">

            <span class="handle ui-sortable-handle">
                <i class="fa fa-ellipsis-v"></i>
                <i class="fa fa-ellipsis-v"></i>
            </span>

            <input type="radio" name="{{ boxLeft.li_input_name }}" value="{{ id }}">
            &nbsp;
            <span class="badge {{ boxLeft.li_badge_class }}">
                {{ id }}
            </span>

            <span class="text">
                {{ entity.name_box|default('< missing name_box >') }}
            </span>

            {#<div class="tools">#}
            {#<i class="fa fa-edit"></i>#}
            {#<i class="fa fa-trash-o"></i>#}
            {#</div>#}
        </li>
    {% else %}
        <li>
            <span class="text">
                No hay resultados.
            </span>
        </li>
    {% endfor %}

{% endspaceless %}
